{
  "name": "socialtechio/auth-service",
  "description": "Repository for register & auth users.",
  "type": "project",
  "require": {
    "php": "^7.1",
    "socialtechio/slow-storage-emulator": "*",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "4.1.*",
    "symfony/console": "4.1.*",
    "symfony/expression-language": "4.1.*",
    "symfony/flex": "^1.1",
    "symfony/form": "4.1.*",
    "symfony/framework-bundle": "4.1.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "*",
    "symfony/process": "4.1.*",
    "symfony/security-bundle": "4.1.*",
    "symfony/serializer-pack": "dev-master",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "4.1.*",
    "symfony/twig-bundle": "4.1.*",
    "symfony/validator": "4.1.*",
    "symfony/web-link": "4.1.*",
    "symfony/yaml": "4.1.*",
    "ramsey/uuid": "^4.0@dev",
    "symfony/messenger": "4.1.*"
  },
  "require-dev": {
    "symfony/debug-pack": "*",
    "symfony/dotenv": "4.1.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/profiler-pack": "*",
    "symfony/test-pack": "*",
    "symfony/web-server-bundle": "4.1.*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Core\\": "core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/socialtechio/slow-storage-emulator.git"
    }
  ],
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.1.*"
    }
  },
  "minimum-stability":"dev",
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
